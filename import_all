#!  /bin/bash 
estimate_quadkeys social/tweets/heap -output database/standard_qkeys


############################################################################################
##### 주요 구역들
############################################################################################
import_shapefile $MARMOT_DATA/행자부/법정구역_5179/시도 구역/시도 -srid EPSG:5179 -geom_col 'the_geom(EPSG:5186)'	\
				-charset euc-kr -f
dataset cluster create 구역/시도
import_shapefile $MARMOT_DATA/행자부/법정구역_5179/시군구 구역/시군구 -srid EPSG:5179 -geom_col 'the_geom(EPSG:5186)'	\
				-charset euc-kr -f
dataset cluster create 구역/시군구
import_shapefile $MARMOT_DATA/행자부/법정구역_5179/읍면동 구역/읍면동 -srid EPSG:5179 -geom_col 'the_geom(EPSG:5186)'	\
				-charset euc-kr -f
dataset cluster create 구역/읍면동
import_shapefile $MARMOT_DATA/행자부/법정구역_5179/리 구역/리 -srid EPSG:5179 -geom_col 'the_geom(EPSG:5186)'	\
				-charset euc-kr -f
dataset cluster create 구역/리
mc_run_module region.ConcatPoliticals
##### 연속지적도_2017
import_shapefile $MARMOT_DATA/공공데이터포털/연속지적도_2017 구역/연속지적도 -geom_col 'the_geom(EPSG:5186)'	\
				-charset euc-kr -f
dataset cluster create 구역/연속지적도 -workers 19
##### 연속지적도_2017 (사업단)
import_shapefile $MARMOT_DATA/사업단자료/연속지적도_2017 구역/연속지적도_2017 -charset utf-8	\
				-geom_col 'the_geom(EPSG:5186)' -f
dataset cluster create 구역/연속지적도_2017 -workers 51
create_thumbnail 구역/연속지적도_2017 1000000
##### 행정동코드
import_shapefile $MARMOT_DATA/사업단자료/행정동코드 구역/행정동코드 -geom_col 'the_geom(EPSG:5186)' -charset utf-8 -f
dataset cluster create 구역/행정동코드 #-block_size 32mb
##### 기초구역
import_shapefile $MARMOT_DATA/행자부/기초구역_5179 구역/기초구역 -srid EPSG:5179 	\
				-geom_col 'the_geom(EPSG:5186)' -charset euc-kr -f
dataset cluster create 구역/기초구역
##### 통계청 집계구
import_shapefile $MARMOT_DATA/사업단자료/통계청집계구 구역/집계구 -geom_col 'the_geom(EPSG:5186)' -charset euc-kr -f
dataset cluster create 구역/집계구
##### 지오비전 집계구
import_shapefile $MARMOT_DATA/사업단자료/지오비전/집계구 구역/지오비전_집계구 -srid EPSG:5186	\
				-geom_col 'the_geom(EPSG:5186)' -f
dataset cluster create 구역/지오비전_집계구
##### 지오비전 집계구 (2018)
dataset import shp  $MARMOT_DATA/사업단자료/지오비전/집계구_2018 구역/지오비전_집계구_2018 -srid EPSG:5186	\
				-geom_col 'the_geom(EPSG:5186)' -charset euc-kr -f
dataset cluster create 구역/지오비전_집계구_2018


############################################################################################
##### 국가공간정보포털 - 국가중점데이터
############################################################################################
##### 연속지적도형정보(국가중점데이터)
import_shapefile $MARMOT_DATA/국가공간정보포털/연속지적도형정보/2019.07 구역/연속지적도_2019 -charset euc-kr	\
				-geom_col 'the_geom(EPSG:5174)' -c lz4 -f
dataset cluster create 구역/연속지적도_2019 -workers 51
create_thumbnail 구역/연속지적도_2019 1000000
##### GIS건물통합정보(국가중점데이터)
